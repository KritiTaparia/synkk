{
    "manifest_version": 3,
    "name": "synkk",
    "description": "Tab syncing Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "images/synkk_icon.jpg"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "tabs",
        "tabGroups"
    ],
    "background": {
        "service_worker": "background.js"
    }
}
